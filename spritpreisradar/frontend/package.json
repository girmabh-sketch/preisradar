{
  "name": "@pascada/spritpreisradar-frontend",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "format": "prettier --write \"src/**/*spec.ts\" ",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "start": "ng serve",
    "start:usemockserver": "ng serve --proxy-config test/proxy/proxy-local.conf.json --port 4202",
    "start:prod": "ng serve --configuration production",
    "mock:server": "json-server --watch test/proxy/db.json --routes test/proxy/routes.json",
    "build": "ng build",
    "build:prod": "ng build --configuration production",
    "install:marlene": "npm install pascada-marlene-0.8.0.tgz",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.2.12",
    "@angular/common": "^18.2.12",
    "@angular/compiler": "^18.2.12",
    "@angular/core": "^18.2.12",
    "@angular/forms": "^18.2.12",
    "@angular/platform-browser": "^18.2.12",
    "@angular/platform-browser-dynamic": "^18.2.12",
    "@angular/router": "^18.2.12",
    "@fortawesome/fontawesome-free": "^6.7.0",
    "@ng-bootstrap/ng-bootstrap": "^17.0.0",
    "@pascada/marlene": "file:pascada-marlene-0.8.0.tgz",
    "bootstrap": "^5.3.3",
    "ng-openlayers": "^19.0.1",
    "ngx-toastr": "^18.0.0",
    "ol": "^8.2.0",
    "rxjs": "*",
    "tslib": "^2.8.1",
    "zone.js": "~0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.2.12",
    "@angular/cli": "^18.2.12",
    "@angular/compiler-cli": "^18.2.12",
    "@angular/localize": "^18.2.12",
    "@eslint/js": "^9.20.0",
    "@types/codemirror": "^5.60.15",
    "@types/jasmine": "^4.0.3",
    "@types/node": "~22.9.1",
    "@typescript-eslint/eslint-plugin": "^8.23.0",
    "@typescript-eslint/parser": "^8.23.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.3",
    "jasmine-core": "~5.4.0",
    "json-server": "^0.17.4",
    "karma": "~6.4.4",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "^2.0.0",
    "prettier": "^3.4.2",
    "typescript": "~5.4.5",
    "typescript-eslint": "^8.23.0"
  }
}
